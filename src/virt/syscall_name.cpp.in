static const char* syscallNames[] = {
// Auto-generated at build time
SYSCALL_NAME_LIST
};

#include <stdint.h>

const char* GetSyscallName(uint32_t syscall) {
    return (syscall >= sizeof(syscallNames)/sizeof(syscallNames[0]))? "INVALID" : syscallNames[syscall];
}

